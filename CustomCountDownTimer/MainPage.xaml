<UserControl x:Class="CustomCountDownTimer.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:thisProject="clr-namespace:CustomCountDownTimer"
    
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400">

    <Grid x:Name="LayoutRoot" Background="White">
        <thisProject:OptionsPaneView/>
        <StackPanel>
            <thisProject:TimerPanel/>
            <StackPanel Orientation="Horizontal">
                <Button Template="{StaticResource CancelButtonTemplate}" Height="48"></Button>
                <Button Template="{StaticResource AcceptButtomTemplate}" Height="48"></Button>
            </StackPanel>
        </StackPanel>
    </Grid>
</UserControl>