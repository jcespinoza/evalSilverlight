<Application xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:customCountDownTimer="clr-namespace:CustomCountDownTimer"
             x:Class="CustomCountDownTimer.App"
             >
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="TemplatesResourceDictionary.xaml">
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
            <customCountDownTimer:StringToDuration x:Key="newConverter" />
        </ResourceDictionary>

    </Application.Resources>
</Application>
